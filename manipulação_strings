var nome = ('Luis Felipe Silva de Melo');

// Manipulação 1:

var maiusc = nome.toUpperCase();
console.log(`Seu nome em letras maiúsculas: ${maiusc}`);

// Manipulação 2:

var minusc = nome.toLowerCase();
console.log(`Seu nome em letras minúsculas: ${minusc}`)

// Manipulação 3:

var tamanho = nome.length
console.log(`Seu nome tem ${tamanho} letras`)
